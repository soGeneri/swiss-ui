{
  "name": "swiss-ui-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "build": "npm run build -w packages/tokens && npm run build -w packages/react",
    "test": "npm run test --workspaces --if-present",
    "dev": "npm run dev -w packages/react",
    "clean": "npm run clean --workspaces --if-present"
  }
}
